{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [{ "source": "/(.*)", "destination": "/" }],
	"git": {
		"deploymentEnabled": {
			"gh-pages": false
		}
	},
	"headers": [
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "cache-control",
					"value": "public,max-age=31536000,immutable"
				}, {
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; connect-src 'self' http://lhr-mnt-rpcbig-05.mainnet.sui.io:9000;"
				} 
			]
		}
	]
}
